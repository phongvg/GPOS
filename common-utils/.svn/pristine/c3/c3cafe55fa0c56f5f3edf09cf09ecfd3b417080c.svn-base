import lombok.Data;
@SuppressWarnings("serial")
@Data
public class ConfigCOModifier implements Serializable{
	@JsonProperty("code")
	private String code;
	@JsonProperty("name-vn")
	private String name;
	@JsonProperty("type")
	private Integer type;
	@JsonProperty("number-of-chili")
	private Integer numberOfChili;
}
